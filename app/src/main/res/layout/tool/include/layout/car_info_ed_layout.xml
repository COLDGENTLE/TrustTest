<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_marginTop="@dimen/qb_px_50"
        android:background="@color/colorWhiteGay"
        android:gravity="center_vertical"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_120">

        <TextView
            android:textColor="@color/colorBlack"
            android:layout_marginLeft="@dimen/qb_px_60"
            android:text="车辆资料"
            android:textSize="@dimen/qb_px_51"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:layout_marginRight="@dimen/qb_px_60"

            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical">

            <EditText
                android:background="@null"
                android:id="@+id/car_info_complete_tx"
                android:textSize="@dimen/qb_px_42"
                android:text="资料完整度"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </LinearLayout>


    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
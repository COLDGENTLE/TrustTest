<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    >

    <TextView
        android:id="@+id/popu_camera_btn"
        android:background="@color/colorWhiteGay"
        android:textColor="@color/colorBlack"
        android:gravity="center"
        android:text="@string/phone_is_resiged_tx"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_158" />
    
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#c4c4c4"
        android:layout_marginLeft="@dimen/qb_px_50"
        android:layout_marginRight="@dimen/qb_px_50"/>




    <TextView
        android:id="@+id/popu_cancel_btn"
        android:background="@color/colorWhiteGay"
        android:textColor="@color/textBlue"
        android:gravity="center"
        android:text="@string/to_login_tx"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_138" />


</LinearLayout>
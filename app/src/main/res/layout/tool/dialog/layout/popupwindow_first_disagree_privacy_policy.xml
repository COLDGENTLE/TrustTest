<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:background="#00000000"
    >



    <RelativeLayout
        android:background="@drawable/white_bg"
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/qb_px_60"
        android:layout_marginRight="@dimen/qb_px_60"
        android:layout_height="@dimen/qb_px_500"
        android:layout_gravity="center">
    <TextView
        android:gravity="center_horizontal"
        android:layout_centerHorizontal="true"
        android:lineSpacingExtra="@dimen/qb_px_10"
        android:layout_marginTop="@dimen/qb_px_90"
        android:id="@+id/popupwindow_disagree_privacy_policy_tx"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



        <TextView
            android:background="@drawable/white_bg"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:id="@+id/privacy_policy_disagree_btn"
            android:gravity="center"
            android:text="@string/popupwindow_disagree_privacy_policy_cancel_tx"
            android:layout_width="@dimen/qb_px_300"
            android:layout_height="@dimen/qb_px_150"
            android:layout_marginBottom="@dimen/qb_px_10" />


    </RelativeLayout>
</RelativeLayout>